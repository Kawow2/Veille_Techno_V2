<Window x:Class="Veille.Screen.GemboxScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Veille.Screen"
        mc:Ignorable="d"
        Title="GemboxScreen" Height="450" Width="800">
    <Grid>
        <Button x:Name="OpenFile" Content="Charger un fichier" HorizontalAlignment="Left" Margin="200,170,0,0" VerticalAlignment="Top" Width="75" Click="OpenFile_Click"/>
        <Button x:Name="WriteFile" Content="Sauvegarder un fichier" HorizontalAlignment="Left" Margin="383,170,0,0" VerticalAlignment="Top" Width="75" Click="WriteFile_Click"/>
        <Label Content="" Name="_FileName" HorizontalAlignment="Left" Margin="215,92,0,0" VerticalAlignment="Top"/>
        <Label Content="" Name="_Time" HorizontalAlignment="Left" Margin="188,296,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
